<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
				<g:Label text="Menu Items"/>
			</g:HorizontalPanel>
		<g:VerticalPanel width="100%" height="" verticalAlignment="ALIGN_TOP">
			<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel spacing="10">
					<g:Button text="New" ui:field="btnNewItem">New menu item</g:Button>
					<g:Button text="Edit" ui:field="btnEditItem" enabled="false"/>
					<g:Button text="Delete" ui:field="btnDeleteItem" enabled="false"/>
				</g:HorizontalPanel>
			</g:HorizontalPanel>
			<g:TabLayoutPanel barHeight="2" barUnit="EM" width="100%" height="100%" ui:field="tabPanelCategories" visible="false"/>
			<g:Label text="Cargando..." ui:field="lblLoading"/>
		</g:VerticalPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 