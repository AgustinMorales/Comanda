							<g:at left="189" top="163">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice6" name="extraPrice6"/>
							</g:at>
							<g:at left="10" top="188">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra7" name="extra7"/>
							</g:at>
							<g:at left="189" top="188">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice7" name="extraPrice7"/>
							</g:at>
							<g:at left="10" top="215">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra8" name="extra8"/>
							</g:at>
							<g:at left="189" top="215">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice8" name="extraPrice8"/>
							</g:at>
							<g:at left="10" top="242">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra9" name="extra9"/>
							</g:at>
							<g:at left="189" top="242">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice9" name="extraPrice9"/>
							</g:at>
							<g:at left="359" top="10">
								<g:Label text="Tipo:"/>
							</g:at>
							<g:at left="408" top="6">
								<g:TextBox ui:field="tbExtrasName" name="extrasName"/>
							</g:at>
						</g:AbsolutePanel>
						<g:HorizontalPanel width="100%"
							horizontalAlignment="ALIGN_RIGHT">
							<g:HorizontalPanel spacing="10">
								<g:Button ui:field="btnCancel">Cancelar</g:Button>
								<g:Button text="Guardar cambios" ui:field="btnSaveChanges" />
							</g:HorizontalPanel>
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:FormPanel>
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 