<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:ScrollPanel width="653px" height="500px">
			<g:FormPanel encoding="ENCODING_MULTIPART" method="METHOD_POST"
				ui:field="itemDataFormPanel" width="652px">
				<g:VerticalPanel width="615px" height="100%">
					<g:HorizontalPanel horizontalAlignment="ALIGN_RIGHT"
						width="100%" verticalAlignment="ALIGN_BOTTOM">
						<g:HorizontalPanel width="100%" spacing="10"
							horizontalAlignment="ALIGN_RIGHT">
							<g:HorizontalPanel width="100%" height="">
								<g:AbsolutePanel height="274px" width="466px">
									<g:at left="10" top="26">
										<g:Label text="Nombre:" />
									</g:at>
									<g:at left="93" top="15">
										<g:TextBox ui:field="tbName" name="itemName" />
									</g:at>
									<g:at left="93" top="105">
										<g:FileUpload width="177px" height="26px"
											ui:field="fuImageFile" name="itemImage" />
									</g:at>
									<g:at left="10" top="110">
										<g:Label text="Imagen:" />
									</g:at>
									<g:at left="336" top="15">
										<g:TextBox visible="false" name="keyId" ui:field="tbKeyId" />
									</g:at>
									<g:at left="10" top="69">
										<g:Label text="Categoría:" />
									</g:at>
									<g:at left="93" top="64">
										<g:ListBox visibleItemCount="1" name="categoryId"
											width="177px" height="27px" ui:field="lbCategory" enabled="false" />
									</g:at>
									<g:at left="10" top="137">
										<g:Label text="Descripción:" />
									</g:at>
									<g:at left="10" top="161">
										<g:TextArea width="436px" height="93px" ui:field="taDescription"
											name="description" />
									</g:at>
								</g:AbsolutePanel>
							</g:HorizontalPanel>
							<g:Image width="155px" height="155px" ui:field="imgItem" />
						</g:HorizontalPanel>
					</g:HorizontalPanel>
					<g:VerticalPanel width="100%" spacing="10">
						<g:Label text="Precios:" />
						<g:AbsolutePanel height="179px" width="632px">
							<g:at left="10" top="10">
								<g:RadioButton name="priceScheme" text="Único"
									formValue="single" checked="true" ui:field="rbSinglePrice" />
							</g:at>
							<g:at left="10" top="35">
								<g:RadioButton name="priceScheme" text="Varios"
									formValue="multiple" ui:field="rbMultiplePrice" />
							</g:at>
							<g:at left="10" top="60">
								<g:AbsolutePanel width="612px" height="110px">
									<g:at left="10" top="13">
										<g:CheckBox text="Pequeña" ui:field="cbSmall" name="cbSmall" />
									</g:at>
									<g:at left="10" top="45">
										<g:CheckBox text="Mediana" ui:field="cbMedium"
											name="cbMedium" />
									</g:at>
									<g:at left="10" top="77">
										<g:CheckBox text="Grande" ui:field="cbLarge" name="cbLarge" />
									</g:at>
									<g:at left="88" top="11">
										<g:DoubleBox ui:field="dbSmallPrice" name="smallPrice" />
									</g:at>
									<g:at left="88" top="43">
										<g:DoubleBox ui:field="dbMediumPrice" name="mediumPrice" />
									</g:at>
									<g:at left="88" top="75">
										<g:DoubleBox ui:field="dbLargePrice" name="largePrice" />
									</g:at>
									<g:at left="331" top="13">
										<g:CheckBox text="Tapa" ui:field="cbTapa" name="cbTapa" />
									</g:at>
									<g:at left="331" top="45">
										<g:CheckBox text="1/2 Ración" ui:field="cbHalf"
											name="cbHalf" />
									</g:at>
									<g:at left="331" top="77">
										<g:CheckBox text="Ración" ui:field="cbFull" name="cbFull" />
									</g:at>
									<g:at left="428" top="9">
										<g:DoubleBox ui:field="dbTapaPrice" name="tapaPrice" />
									</g:at>
									<g:at left="428" top="45">
										<g:DoubleBox ui:field="dbHalfPrice" name="halfPrice" />
									</g:at>
									<g:at left="428" top="77">
										<g:DoubleBox ui:field="dbFullPrice" name="fullPrice" />
									</g:at>
								</g:AbsolutePanel>
							</g:at>
							<g:at left="96" top="8">
								<g:DoubleBox ui:field="dbSinglePrice" name="singlePrice" />
							</g:at>
						</g:AbsolutePanel>
						<g:Label text="Extras:"/>
						<g:AbsolutePanel height="275px">
							<g:at left="10" top="30">
								<g:TextBox width="159px" height="7px" name="extra1" ui:field="tbExtra1"/>
							</g:at>
							<g:at left="10" top="6">
								<g:Label text="Nombre"/>
							</g:at>
							<g:at left="189" top="6">
								<g:Label text="Precio"/>
							</g:at>
							<g:at left="10" top="57">
								<g:TextBox width="159px" height="7px" name="extra2" ui:field="tbExtra2"/>
							</g:at>
							<g:at left="189" top="57">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice2" name="extraPrice2"/>
							</g:at>
							<g:at left="10" top="84">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra3" name="extra3"/>
							</g:at>
							<g:at left="189" top="84">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice3" name="extraPrice3"/>
							</g:at>
							<g:at left="189" top="30">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice1" name="extraPrice1"/>
							</g:at>
							<g:at left="10" top="109">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra4" name="extra4"/>
							</g:at>
							<g:at left="189" top="109">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice4" name="extraPrice4"/>
							</g:at>
							<g:at left="10" top="136">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra5" name="extra5"/>
							</g:at>
							<g:at left="189" top="136">
								<g:DoubleBox width="123px" height="13px" ui:field="tbExtraPrice5" name="extraPrice5"/>
							</g:at>
							<g:at left="10" top="163">
								<g:TextBox width="159px" height="7px" ui:field="tbExtra6" name="extra6"/>
