<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%">
			<g:HorizontalPanel width="100%"
				horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_BOTTOM">
				<g:HorizontalPanel spacing="10">
					<g:Button text="Crear nuevo" ui:field="btnNewRestaurant" />
					<g:Button text="Editar" ui:field="btnEditRestaurant"
						enabled="false" />
					<g:Button ui:field="btnCopy" enabled="false">Copiar</g:Button>
					<g:Button text="Eliminar" ui:field="btnDeleteRestaurant"
						enabled="false" />
				</g:HorizontalPanel>
			</g:HorizontalPanel>
			<g:VerticalPanel width="100%" ui:field="restaurantsTableContainer">
				<g:ScrollPanel height="290px">
					<p1:CellTable ui:field="restaurantsTable" width="100%"
						height="100%" />
				</g:ScrollPanel>
				<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER"
					width="100%">
					<p1:SimplePager location="CENTER" ui:field="restaurantsPager" />
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 